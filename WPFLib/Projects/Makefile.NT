LIB=$(OUTDIR)\$(PROJECT_TARGET).lib

projectall: enums registration $(LIB)

enums: ..\src\AVUIGenEnums.h ..\src\AVUIGenEnums.cpp

registration:
	..\tools\GenRegisterTypes.pl ..\src $(OBJDIR) > ..\src\AVUIGenRegistration.cpp

..\src\AVUIGenEnums.h: ..\src\Enums.xml ..\Tools\GenEnumHeader.pl
	..\Tools\GenEnumHeader.pl ..\src\Enums.xml > ..\include\AVUIGenEnums.h

..\src\AVUIGenEnums.cpp: ..\src\Enums.xml ..\Tools\GenEnumBody.pl
	..\Tools\GenEnumBody.pl ..\src\Enums.xml > ..\src\AVUIGenEnums.cpp
	
$(LIB): $(OBJ)
	lib $(LIBFLAGS) $(OBJ) /OUT:$(LIB)

{..\src\tess}.cpp{$(OBJDIR)}.obj:
	cl /Zi /c $(CFLAGS) $< /Fo$@


